{
  "manifest_version": 3,
  "name": "BrokerPilot Stands Manager",
  "version": "1.0.1",
  "description": "Управление тестовыми стендами BrokerPilot",
  "permissions": ["storage"],
  "host_permissions": ["http://*/*"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "BrokerPilot Stands Manager",
    "default_icon": "icon.png"
  },
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  }
}
